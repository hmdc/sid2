---
support_ticket:
  rt_api:
    server: "<%= @rt_server %>"
    user: "<%= @rt_api_user %>"
    pass: "<%= @rt_api_pass %>"
    priority: "<%= @rt_priority %>"
  <% if @rt_queues.length -%>
    queues:
  <% @rt_queues.each do |queue| -%>
      - "<%= queue %>"
  <% end -%>
  <% end -%>
  <% unless @rt_proxy_server.nil? -%>
    proxy: "<%= @rt_proxy_server %>"
  <% end -%>