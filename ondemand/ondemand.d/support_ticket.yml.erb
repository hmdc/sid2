---
support_ticket:
  rt_api:
    server: "<%= @rt_server %>"
    user: "<%= @rt_api_user %>"
    pass: "<%= @rt_api_pass %>"
<% if @rt_queues.length -%>
  queues:
<% @rt_queues.each do |queue| -%>
    - "<%= queue %>"
<% end -%>
<% end -%>
<% unless @proxy_server.nil? -%>
  proxy: "<%= @proxy_server %>"
<% end -%>