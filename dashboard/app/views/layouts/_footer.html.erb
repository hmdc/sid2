<footer>
  <div class="container-fluid" 
    <div class="row">
      <div class="col-md-2 col-sm-4">
        <%= link_to "https://osc.github.io/Open-OnDemand/" do %>
          <%= image_tag(
                "OpenOnDemand_powered_by_RGB.svg",
                class: "footer-logo img-responsive center-block",
                alt: "Powered by Open OnDemand",
                height: "40",
                style: "margin-bottom: 20px; max-height: 40px;"
              ) %>
        <% end %>
      </div>
      <div class="col-md-2 col-sm-4">
        <%= link_to "https://vdi.rc.fas.harvard.edu/pun/sys/dashboard" do %>
          <%= image_tag(
                "fasrc_logo.jpg",
                class: "footer-logo img-responsive center-block",
                alt: "FASRC",
                height: "40",
                style: "margin-bottom: 20px; max-height: 40px;"
              ) %>
        <% end %>
      </div>
      <div class="col-md-2 col-sm-4">
        <%= link_to "https://www.iq.harvard.edu/" do %>
          <%= image_tag(
                "iqss_logo.png",
                class: "footer-logo img-responsive center-block",
                alt: "IQSS",
                height: "40",
                style: "margin-bottom: 20px; max-height: 40px;"
              ) %>
        <% end %>
      </div>
  </div><!-- /.container -->
